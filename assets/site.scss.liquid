.btn-primary {
color: #FFF;
    font-size: 17px;
    text-transform: uppercase;
    display: inline-block;
    font-family: Work Sans, "HelveticaNeue", "Helvetica Neue", sans-serif;
    font-weight: 700;
    padding: 15px 15px 15px 60px;
    background: #f90;
    background: -moz-linear-gradient(left, #f90 0%, #f7a900 100%);
    background: -webkit-gradient(left top, right top, color-stop(0%, #f90), color-stop(100%, #f7a900));
    background: -webkit-linear-gradient(left, #f90 0%, #f7a900 100%);
    background: -o-linear-gradient(left, #f90 0%, #f7a900 100%);
    background: -ms-linear-gradient(left, #f90 0%, #f7a900 100%);
    background: linear-gradient(to right, #f90 0%, #f7a900 100%);
    border-radius: 5px;
    position: relative;
    transition: all 0.5s ease;
    &:before {
      content: url({{ 'knack_icon.png' | asset_url }});
      height: 100%;
      position: absolute;
      left: 0;
      top: 0;
      bottom: 0;
      background: rgba(250,250,250,.2);
      border-radius: 4px 0 0 4px;
      padding: 15px 10px;

    }
  &:hover {
    background: #f90;
    background: -moz-linear-gradient(left, #f7a900 0%, #f90 100%);
    background: -webkit-gradient(left top, right top, color-stop(0%, #f7a900), color-stop(100%, #f90));
    background: -webkit-linear-gradient(left, #f7a900 0%, #f90 100%);
    background: -o-linear-gradient(left, #f7a900 0%, #f90 100%);
    background: -ms-linear-gradient(left, #f7a900 0%, #f90 100%);
    background: linear-gradient(to right, #f7a900 0%, #f90 100%);
    color: #FFF;
  }
  &.disabled {
    opacity: .4;

  }
  &.input {
    padding: 15px;
  }
  &.full {
  width: 100%;
  }
  &.btn--small {
    padding: 10px 15px;
    font-size: 15px;
    &:before {
      content: "";
      padding: 0;
    }
  }
  &.address-new-toggle {
    float: right;
  }
}

