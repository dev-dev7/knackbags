.btn--loading {
  position: relative;
  span {
    opacity: 0;
  }
  &:after {
    content: '';
    display: block;
    width: 24px;
    height: 24px;
    position: absolute;
    left: 55%;
    top: 50%;
    margin-left: -12px;
    margin-top: -12px;
    border-radius: 50%;
    border: 3px solid #FFF;
    border-top-color: transparent;
    -webkit-animation: spin 1s infinite linear;
            animation: spin 1s infinite linear;
  }
}
