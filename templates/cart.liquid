    }

    .affirm-as-low-as.hide-modal-trigger .affirm-modal-trigger{
        display: none;
    }

    .remove-link{
        text-align: left;
    }

    .cart__subtotal{
        font-size: 22px;
    }

    .continue-shopping {
        margin-bottom: .5rem;
    }


    .tax-note{
        text-align: right;
    }

    @media only screen and (max-width: 750px) {

        .additional_checkout_buttons ul li{
            width: 100%;
        }

        .tax-note {
            text-align: left;
            font-size: 14px;
        }

        .cart-checkout .sticky{
            position: sticky;
            top: 0;
        }

        .remove-link *{
            text-align: right;
        }

        .line-details {
            padding-left: 4px;
        }

        .returns-callout {
