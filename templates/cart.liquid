        font-family: "Lora";
        color: rgb(5, 5, 5);
        line-height: 1.042;
        margin-bottom: 1rem;
    }

    .continue-shopping a {
        text-decoration: underline;
    }

    .cart-title {
        font-family: "Lora";
        font-size: 36px;
        color: rgb(5, 5, 5);
        text-transform: none;
        font-weight: 400;
    }

    .cart__row {
        margin-bottom: 1rem;
        border-bottom: 1px solid #e8e8e1;
    }




    .cart-checkout .btn{
        margin-top: .5rem !important;
        margin-bottom: 0rem !important;
        height: 44px;
        text-transform: none !Important;
        color: #000;
        font-weight: 500;
        font-size: 18px;
    }

    .cart__row .btn {
        margin-top: .5rem !important;
        margin-bottom: 0rem !important;
        height: 44px;
        text-transform: none !Important;
        color: #000;
        font-weight: 500;
        font-size: 18px;
    }

    .cart__row .line-details * {
        font-size: 15px;

    }

    .cart__row .line-details  .cart__product-name{
        font-size: 18px;
    }

    .cart__row .line-details  .cart__product-meta{
        font-size: 18px;
    }

    .cart__row .line-details  .remove-link{
        font-size: 14px;
    }

    .cart__row .line-details  .item-price *{
        font-size: 18px;
    }

    .cls-1{
        stroke: #525252 !important;
    }



    .additional_checkout_buttons {
        margin-left: 0;
    }

    .additional_checkout_buttons ul{
        display: flex;
        justify-content: flex-end;
