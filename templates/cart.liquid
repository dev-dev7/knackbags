<div class="page-width page-content pt-2 pt-lg-4">

    {% include 'breadcrumbs' %}



    <div class="row no-gutters">
        <div class="col-12 col-lg-8 divider-lg-right">
            <div>
                <header class="section-header">
                    <div class="congrats-message">
                        {% if cart.total_price > 5000 %}
                            <b>Congratulations!</b> You qualify for <b>Free Ground Shipping</b>.

                        {% else %}
                            Free Ground Shipping On All Orders Over $50.
                        {% endif %}
                    </div>
                    <div class="d-block d-md-flex align-items-end justify-content-between mb-3 mb-md-1">
                        <h1 class="section-header__title cart-title"> Shopping Cart</h1>
                        <div class="continue-shopping">
                            <a onclick="goBack()">Continue Shopping ></a>
                        </div>
                    </div>

                </header>
            </div>
            <div>
                {% if cart.item_count > 0 %}


                    <form action="/cart" method="post" novalidate class="cart">
                        <div class="cart__row">
                            <div class="d-block d-lg-none justify-content-between align-items-end mt-3">
                                <div class="cart-subtotal">
                                    <div class="d-flex">
                                        <div class="mr-2">
                                            <p class="cart__subtotal mb-0 font-weight-medium">{{ 'cart.general.subtotal' | t }}:</p>
                                        </div>

                                        <div>
                                            <p class="cart__subtotal mb-0"><span class="wh-original-cart-total"><span
                                                        class='wh-original-price'>{{ cart.total_price | money }}</span></span><span
                                                    class="wh-cart-total"></span>
                                            </p>
                                        </div>
                                    </div>
                                </div>
                                <div class="d-none">
                                    <p class="affirm-as-low-as hide-modal-trigger" data-page-type="cart" data-amount="{{ cart.total_price }}"></p>
                                </div>
                                <div class="cart-checkout sticky">
                                    <button type="submit" name="checkout"
                                            class="w-100 btn btn-primary btn--small mt-0 mb-0 cart__checkout{% if settings.cart_terms_conditions_enable %} cart__checkout--page{% endif %}">
                                        Proceed to Checkout
                                    </button>
                                </div>
                            </div>
                        </div>
                        {% comment %}
                        <div class="cart__row small--hide cart__header-labels d-none">
                          <div class="grid--full">
                            <div class="grid__item medium-up--two-fifths medium-up--push-three-fifths">
                              <div class="grid--full">
                                <div class="grid__item one-third small--half text-center">
                                  {{ 'cart.label.quantity' | t }}
                                </div>
                                <div class="grid__item two-thirds small--half text-right">
                                  {{ 'cart.label.total' | t }}
                                </div>
                              </div>
                            </div>
                          </div>
                        </div>
                        {% endcomment %}

                        {% for item in cart.items %}
                            <div class="cart__row">
                                <div class="row no-gutters align-items-center">
                                    <div class="col-4 col-lg-3 line-image">
                                        <a href="{{ item.url | within: collections.all }}" class="cart__image">
                                            {%- assign img_url = item | img_url: '200x200' -%}
                                            <img class="lazyload pr-1"
                                                 data-src="{{ img_url }}"
                                                 data-widths="[180, 360, 540]"
                                                 data-sizes="auto"
                                                 alt="{{ item.title | escape }}">
                                        </a>


                                    </div>
                                    <div class="col-1 d-none d-lg-block"></div>
                                    <div class="col-8 line-details">
                                        <div class="mb-2">
                                            <div class="mb-1">
                                                <a href="{{ item.url }}" class="cart__product-name">
                                                    {{ item.product.title }}
                                                    <span class='booster-cart-item-success-notes'
                                                          data-key='{{ item.key }}'></span><span
                                                        class='booster-cart-item-upsell-notes'
                                                        data-key='{{ item.key }}'></span>
                                                </a>
                                                {% unless item.variant.title contains 'Default' %}
                                                    <p class="cart__product-meta">
                                                        Color: {{ item.variant.title }}
                                                    </p>
                                                {% endunless %}
                                            </div>
                                            <div class="d-none">
                                                {% render 'judgeme_widgets', widget_type: 'judgeme_preview_badge', concierge_install: false, product: item.product %}
                                            </div>



                                            {% assign property_size = item.properties | size %}
                                            {% if property_size > 0 %}
                                                {% for p in item.properties %}
                                                    {% assign first_character_in_key = p.first | truncate: 1, '' %}
                                                    {% unless p.last == blank or first_character_in_key == '_' %}
                                                        {{ p.first }}:

                                                        {% if p.last contains '/uploads/' %}
                                                            <a href="{{ p.last }}">{{ p.last | split: '/' | last }}</a>
                                                        {% else %}
                                                            {{ p.last }}
                                                        {% endif %}

                                                        <br>
                                                    {% endunless %}
                                                {% endfor %}
                                            {% endif %}


                                        </div>
                                        <div class="mb-4" style="max-width: 240px;">
                                            <div class="d-flex align-items-center item-price">
                                                {% if item.variant.compare_at_price and item.variant.compare_at_price != item.variant.price %}
                                                    <div class="flex">
                                                        <span class="cart__price cart__price--strikethrough">{{ item.variant.compare_at_price | money }}</span>
                                                        <span class="cart__price">{{ item.variant.price | money }}</span>
                                                    </div>
                                                {% else %}
                                                    <div class="flex">
                                                        <span class="cart__price">{{ item.variant.price | money }}</span>
                                                    </div>
                                                {% endif %}
                                                <div class="text-center mr-2 ml-2 font-sans">
                                                    x
                                                </div>
                                                <div>
                                                    <label for="updates_{{ item.key }}"
                                                           class="visually-hidden">{{ 'cart.label.quantity' | t }}</label>
                                                    <input type="number" name="updates[]" id="updates_{{ item.key }}"
                                                           class="cart__product-qty" value="{{ item.quantity }}" min="0"
                                                           data-id="{{ item.key }}">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="d-flex" style="max-width: 240px;">
                                            <div class="mr-1 flex">
                                                {% if item.product.tags contains 'no-returns' %}
                                                    <a class="returns-callout font-weight-bold" data-remodal-target="asIsModal">
                                                        Sold As-Is
                                                        <svg class="ui-icon -icon-controls-pop-in-new" id="sprite-controls-pop-in-new" viewBox="0 0 16 16"><path fill-rule="evenodd" d="M4.567 1.8v9.6h9.6V1.8h-9.6zM3 4.667v8.2h8.2v1.4H1.6v-9.6H3zM12.767 3.2V10h-6.8V3.2h6.8z"></path></svg>
                                                    </a>
                                                {% else %}
                                                    <a class="returns-callout font-weight-bold" data-remodal-target="returnsModal">
                                                        Free Returns
                                                        <svg class="ui-icon -icon-controls-pop-in-new" id="sprite-controls-pop-in-new" viewBox="0 0 16 16"><path fill-rule="evenodd" d="M4.567 1.8v9.6h9.6V1.8h-9.6zM3 4.667v8.2h8.2v1.4H1.6v-9.6H3zM12.767 3.2V10h-6.8V3.2h6.8z"></path></svg>
                                                    </a>
                                                {% endif %}
                                            </div>
                                            <div>
                                                <div class="cart__product-meta remove-link">
                                                    <a href="/cart/change?line={{ forloop.index }}&amp;quantity=0"
                                                       class="">
                                                        Remove Item
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            {% comment %}
                          <div class="cart__row d-none">

                            <div class="row">
                              <div class="flex"></div>
                            </div>

                            <div class="grid--full cart__row--table-large">

                              <div class="grid__item medium-up--three-fifths">
                                <div class="grid">

                                  <div class="grid__item one-quarter">
                                    <a href="{{ item.url | within: collections.all }}" class="cart__image">
                                      {%- assign img_url = item | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
                                      <img class="lazyload"
                                          data-src="{{ img_url }}"
                                          data-widths="[180, 360, 540]"
                                          data-aspectratio="{{ item.aspect_ratio }}"
                                          data-sizes="auto"
                                          alt="{{ item.title | escape }}">
                                    </a>
                                  </div>

                                  <div class="grid__item three-quarters">
                                    <a href="{{ item.url }}" class="cart__product-name">
                                      {{ item.product.title }}
                <span class='booster-cart-item-success-notes' data-key='{{item.key}}'></span><span class='booster-cart-item-upsell-notes' data-key='{{item.key}}'></span>
                                    </a>
                                    {% unless item.variant.title contains 'Default' %}
                                      <p class="cart__product-meta">
                                        {{ item.variant.title }}
                                      </p>
                                    {% endunless %}

                                    {% assign property_size = item.properties | size %}
                                    {% if property_size > 0 %}
                                      {% for p in item.properties %}
                                        {% assign first_character_in_key = p.first | truncate: 1, '' %}
                                        {% unless p.last == blank or first_character_in_key == '_' %}
                                          {{ p.first }}:

                                          {% if p.last contains '/uploads/' %}
                                            <a href="{{ p.last }}">{{ p.last | split: '/' | last }}</a>
                                          {% else %}
                                            {{ p.last }}
                                          {% endif %}

                                          <br>
                                        {% endunless %}
                                      {% endfor %}
                                    {% endif %}

                                    <p class="cart__product-meta">
                                      <a href="/cart/change?line={{ forloop.index }}&amp;quantity=0" class="btn btn-secondary btn--small">
                                        {{ 'cart.general.remove' | t }}
                                      </a>
                                    </p>
                                  </div>

                                </div>
                              </div>

                              <div class="grid__item medium-up--two-fifths">
                                <div class="grid--full cart__row--table">

                                  <div class="grid__item one-third text-center">

                                    <label for="updates_{{ item.key }}" class="visually-hidden">{{ 'cart.label.quantity' | t }}</label>
                                    <input type="number" name="updates[]" id="updates_{{ item.key }}" class="cart__product-qty" value="{{ item.quantity }}" min="0" data-id="{{ item.key }}">

                                  </div>

                                  <div class="grid__item two-thirds text-right">
                                    {% if item.original_line_price != item.line_price %}
                                      <small class="cart__price cart__price--strikethrough">{{ item.original_line_price | money }}</small>
                                    {% endif %}

                                    <span class="cart__price">{{ item.price | money }}</span>

                                    {% assign discounts_size = item.discounts | size %}
                                    {% if discounts_size > 0 %}
                                      {% for discount in item.discounts %}
                                        <small class="cart__discount">{{ discount.title }}</small>
                                      {% endfor %}
                                    {% endif %}
                                  </div>
                                </div>
                              </div>

                            </div>
                          </div>
                            {% endcomment %}
                        {% endfor %}

                        <div class="cart__row border-0">
                            <div class="cart-subtotal">
                                <div class="d-flex justify-content-lg-end">
                                    <div class="mr-2">
                                        <p class="cart__subtotal mb-0 font-weight-medium">{{ 'cart.general.subtotal' | t }}:</p>
                                    </div>
                                    <div>
                                        <p class="cart__subtotal mb-0"><span class="wh-original-cart-total"><span
                                                    class='wh-original-price'>{{ cart.total_price | money }}</span></span><span
                                                class="wh-cart-total"></span>
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="cart__row border-0">
                            <div class="d-flex justify-content-lg-end">
                                <p class="affirm-as-low-as hide-modal-trigger" data-page-type="cart" data-amount="{{ cart.total_price }}"></p>
                            </div>
                        </div>
                        <div>
                            <div class="tax-note">
                                Shipping and taxes are calculated at checkout.

                            </div>
                        </div>
                        <div class="cart__row border-0">
                            <div class="d-block d-lg-flex justify-content-end mb-4">
                                <div class="flex-1"></div>
                                <button type="submit" name="update"
                                        class="btn btn-secondary btn--small update-cart">{{ 'cart.general.update' | t }}</button>
                                <button type="submit" name="checkout"
                                        class="btn btn-primary btn--small cart__checkout{% if settings.cart_terms_conditions_enable %} cart__checkout--page{% endif %}">
                                    Proceed to Checkout
                                </button>
                            </div>


                            {% if additional_checkout_buttons and settings.cart_type != 'drawer' %}
                                <div class="additional_checkout_buttons">{{ content_for_additional_checkout_buttons }}</div>
                            {% endif %}
                        </div>


                        <div class="cart__row d-none">
                            <div class="grid--full cart__row--table-large">
                                {% comment %}
                                  Optional, add a textarea for special notes
                                    - Your theme settings can turn this on or off. Default is on.
                                    - Make sure you have name="note" for the message to be submitted properly
                                {% endcomment %}
                                {% if settings.cart_notes_enable %}
                                    <div class="grid__item medium-up--seven-twelfths">
                                        <div class="cart__note">
                                            <label for="CartSpecialInstructions">{{ 'cart.general.note' | t }}</label>
                                            <textarea name="note" class="input-full"
                                                      id="CartSpecialInstructions">{{ cart.note }}</textarea>
                                        </div>
                                    </div>
                                {% endif %}
                                <div class="">
                                    <div class="grid--full cart__row--table">
                                        <div class="grid__item one-half medium-up--two-thirds medium-up--text-center">
                                            <p class="h4 cart__subtotal">{{ 'cart.general.subtotal' | t }}</p>
                                        </div>
                                        <div class="grid__item one-half medium-up--one-third">
                                            <p class="h4 cart__subtotal"><span class="wh-original-cart-total"><span
                                                        class='wh-original-price'>{{ cart.total_price | money }}</span></span><span
                                                    class="wh-cart-total"></span>
                                            <div class="additional-notes"><span class="wh-minimums-note"></span><span
                                                    class="wh-extra-note"></span></div>
                                            </p>
                                        </div>
                                    </div>

                                    {% if cart.total_discounts > 0 %}
                                        {% assign cartTotalDiscounts = cart.total_discounts | money %}
                                        <p class="cart__savings">{{ 'cart.general.savings_html' | t: savings: cartTotalDiscounts }}</p>
                                    {% endif %}
                                    <small>
                                        {{ 'cart.general.shipping_at_checkout' | t }}<br/>
                                        {% if settings.enable_currencies %}
                                            {{ 'cart.general.orders_processed_in_currency_html' | t: currency: shop.currency }}
                                        {% endif %}
                                    </small>

                                    <div class="d-flex justify-content-end">
                                        <p class="affirm-as-low-as" data-page-type="cart"
                                           data-amount="{{ cart.total_price }}"></p>
                                    </div>
                                    <div>
                                        {% if settings.cart_terms_conditions_enable %}
                                            <p class="ajaxcart__note ajaxcart__note--terms">
                                                <input type="checkbox" id="CartPageAgree"/>
                                                <label for="CartPageAgree">
                                                    {% if settings.cart_terms_conditions_link != blank %}
                                                        {{ 'cart.general.terms_html' | t: url: settings.cart_terms_conditions_link }}
                                                    {% else %}
                                                        {{ 'cart.general.terms' | t }}
                                                    {% endif %}
                                                </label>
                                            </p>
                                        {% endif %}
                                    </div>
                                    <div class="d-flex justify-content-end">
                                        <button type="submit" name="update"
                                                class="btn btn-secondary btn--small update-cart">{{ 'cart.general.update' | t }}</button>
                                        <button type="submit" name="checkout"
                                                class="btn btn-primary btn--small cart__checkout{% if settings.cart_terms_conditions_enable %} cart__checkout--page{% endif %}">
                                            {{ 'cart.general.checkout' | t }}
                                        </button>
                                    </div>


                                    {% if additional_checkout_buttons and settings.cart_type != 'drawer' %}
                                        <div class="additional_checkout_buttons">{{ content_for_additional_checkout_buttons }}</div>
                                    {% endif %}
                                </div>
                            </div>

                        </div>

                    </form>

                {% else %}
                    <div>
                        <p>{{ 'cart.general.empty' | t }}</p>
                        <p class="rte">{{ 'cart.general.continue_browsing_html' | t }}</p>
                    </div>
                {% endif %}
            </div>
        </div>
        <div class="col-12 col-lg-4 p-4 sidebar">
            <form action="/cart" method="post" novalidate class="cart">
                <div class="sticky">
                    <div class="d-none d-lg-block justify-content-between align-items-end">
                        <div class="cart-subtotal">
                            <div class="d-flex">
                                <div class="mr-2">
                                    <p class="cart__subtotal mb-0 font-weight-medium">{{ 'cart.general.subtotal' | t }}:</p>
                                </div>
                                <div>
                                    <p class="cart__subtotal mb-0"><span class="wh-original-cart-total"><span
                                                class='wh-original-price'>{{ cart.total_price | money }}</span></span><span
                                            class="wh-cart-total"></span>
                                    </p>
                                </div>
                            </div>
                        </div>
                        <div>
                            <div class="d-flex justify-content-lg-end">
                                <p class="affirm-as-low-as" data-page-type="cart" data-amount="{{ cart.total_price }}"></p>
